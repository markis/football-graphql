{
  "name": "fantasy-football-graphql",
  "description": "Graphql server for yahoo fantasy football",
  "version": "1.0.0",
  "scripts": {
    "build": "npx tsc && npm run build:script && npm run build:cleanup",
    "build:script": "npx rollup -c",
    "build:cleanup": "rm -rf ./tmp/",
    "debug": "npm run build && node --nolazy --inspect-brk=5858 ./index.js",
    "start": "npm run build && node ./index.js",
    "start:watch": "nodemon -x yarn start -e .ts"
  },
  "dependencies": {
    "@markis/stattleship": "^0.0.7-0",
    "@types/body-parser": "^1.16.5",
    "@types/config": "^0.0.32",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.37",
    "@types/express-graphql": "^0.0.33",
    "@types/express-session": "^1.15.3",
    "@types/graphql": "^0.11.0",
    "@types/method-override": "^0.0.30",
    "@types/passport": "^0.3.4",
    "@types/simple-oauth2": "^1.0.3",
    "body-parser": "^1.17.2",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.0",
    "dataloader": "^1.3.0",
    "express": "^4.15.4",
    "express-graphql": "^0.6.7",
    "express-session": "^1.15.5",
    "graphql": "^0.11.2",
    "method-override": "^2.3.9",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "passport-oauth2-refresh": "^1.0.0",
    "passport-yahoo-oauth": "^0.1.2",
    "simple-oauth2": "^1.3.0",
    "stattleship": "file:../stattleship",
    "yahoo-fantasy": "^2.0.4"
  }
}
