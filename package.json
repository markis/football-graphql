{
  "name": "fantasy-football-graphql",
  "description": "Graphql server for yahoo fantasy football",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && npm run build:script && npm run build:cleanup",
    "build:script": "rollup -c",
    "build:cleanup": "rm -rf ./tmp/",
    "debug": "npm run build && node --nolazy --inspect-brk=5858 ./index.js",
    "start": "npm run build && node ./index.js",
    "start:watch": "nodemon -x yarn start -e .ts",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@alexlur/rollup-plugin-typescript": "^1.0.4",
    "@markis/stattleship": "0.0.9-0",
    "@types/body-parser": "^1.16.5",
    "@types/config": "^0.0.32",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/express-graphql": "^0.0.33",
    "@types/express-session": "^1.15.3",
    "@types/graphql": "^0.10.2",
    "@types/method-override": "^0.0.30",
    "@types/passport": "^0.3.4",
    "@types/simple-oauth2": "^1.0.3",
    "body-parser": "^1.18.2",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.2",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "express": "^4.16.0",
    "express-graphql": "^0.6.7",
    "express-session": "^1.15.6",
    "graphql": "^0.10.5",
    "graphql-tag": "^2.4.2",
    "method-override": "^2.3.10",
    "newrelic": "^4.0.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "passport-oauth2-refresh": "^1.0.0",
    "passport-yahoo-oauth": "^0.1.2",
    "rollup": "^0.49.2",
    "rollup-plugin-graphql": "^0.1.0",
    "simple-oauth2": "^1.3.0",
    "typescript": "^2.5.2",
    "yahoo-fantasy": "^2.0.4",
    "snyk": "^1.305.0"
  },
  "snyk": true
}
